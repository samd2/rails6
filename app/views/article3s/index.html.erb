<p id="notice"><%= notice %></p>

<h1>Article3s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article3s.each do |article3| %>
      <tr>
        <td><%= article3.title %></td>
        <td><%= article3.body %></td>
        <td><%= link_to 'Show', article3 %></td>
        <td><%= link_to 'Edit', edit_article3_path(article3) %></td>
        <td><%= link_to 'Destroy', article3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article3', new_article3_path %>
